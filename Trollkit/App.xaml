<Application x:Class="Trollkit.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionaries\MainWindowDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries\WindowsTrollsDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries\VisualTrollsDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries\ClientsDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries\AudioTrollsDictionary.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries\SystemInfoDictionary.xaml"/>
				<ResourceDictionary Source="ResourceDictionaries\CMDShellDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>