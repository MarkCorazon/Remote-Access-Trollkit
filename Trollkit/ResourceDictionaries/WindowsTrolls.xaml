                <VisualBrush TileMode="Tile">
                    <VisualBrush.Visual>
                        <StackPanel>
                            <StackPanel.Background>
                                <DrawingBrush>
                                    <DrawingBrush.Drawing>
                                        <GeometryDrawing>

                                            <GeometryDrawing.Brush>
                                                <LinearGradientBrush StartPoint="1,0" EndPoint="1,1">
                                                    <GradientStop Color="#129566" Offset="0.0" />
                                                    <GradientStop Color="#c71494" Offset="1" />
                                                </LinearGradientBrush>
                                            </GeometryDrawing.Brush>

                                            <GeometryDrawing.Geometry>
                                                <GeometryGroup>
                                                    
                                                    <GeometryGroup>
                                                        <GeometryGroup.Transform>
                                                            <TransformGroup>
                                                                <TranslateTransform X="0.4" Y="0.5"/>
                                                                <ScaleTransform ScaleX="0.468" ScaleY="0.468"/>
                                                            </TransformGroup>
                                                        </GeometryGroup.Transform>
                                                        <PathGeometry FillRule="Nonzero" Figures="M60.2,85.3L72.5,43c4.6-2.1,16.7-6.7,31.3-3.3c7,1.6,12.4,4.6,16,7c-4.3,14.4-8.7,28.9-13,43.3c-2.8-2.5-9.2-7.8-19-9.7C73.9,77.6,63,83.7,60.2,85.3z"/>
