                                                        <PathGeometry FillRule="Nonzero" Figures="M173.2,54.1l-12.3,42.3c-4.6,2.1-16.7,6.7-31.3,3.3c-7-1.6-12.4-4.6-16-7c4.3-14.4,8.7-28.9,13-43.3c2.8,2.5,9.2,7.8,19,9.7C159.5,61.8,170.3,55.8,173.2,54.1z"/>
                                                    </GeometryGroup>

                                                    <GeometryGroup>
                                                        <GeometryGroup.Transform>
                                                            <TransformGroup>
                                                                <TranslateTransform X="0.5" Y="0.5"/>
                                                                <ScaleTransform ScaleX="1" ScaleY="1"/>
                                                            </TransformGroup>
                                                        </GeometryGroup.Transform>
                                                        <RectangleGeometry Rect="0,0,100,8" />
                                                        <RectangleGeometry Rect="0,10,100,8" />
                                                        <RectangleGeometry Rect="0,20,100,8" />
                                                        <RectangleGeometry Rect="0,30,100,8" />
                                                        <RectangleGeometry Rect="0,40,100,8" />
                                                        <RectangleGeometry Rect="0,50,100,8" />
                                                        <RectangleGeometry Rect="0,60,100,8" />
                                                        <RectangleGeometry Rect="0,70,100,8" />
                                                        <RectangleGeometry Rect="0,80,100,8" />
                                                        <RectangleGeometry Rect="0,90,100,10" />
                                                    </GeometryGroup>
                                                </GeometryGroup>
                                            </GeometryDrawing.Geometry>
                                            
                                            

                                        </GeometryDrawing>
                                    </DrawingBrush.Drawing>
                                </DrawingBrush>
                            </StackPanel.Background>
                            <TextBlock FontSize="10pt" Margin="10"></TextBlock>
                        </StackPanel>
                    </VisualBrush.Visual>
                </VisualBrush>
            </Setter.Value>
        </Setter>
    </Style>




    <Style x:Key="VerticalGlitchText" TargetType="{x:Type Label}">
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontSize" Value="60"/>

        <Setter Property="Padding" Value="0,40,60,40"/>
        
        <Setter Property="Width" Value="auto"/>

        <Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect
                ShadowDepth="2"
                Direction="90"
                Color="#c71494"
                Opacity="1"
                 BlurRadius="0"/>
            </Setter.Value>
        </Setter>

        <Setter Property="LayoutTransform">
            <Setter.Value>
                <TransformGroup>
                    <RotateTransform Angle="-90" />
                </TransformGroup>
            </Setter.Value>
            
        </Setter>
    </Style>

    <Style x:Key="OutrunBigButton" TargetType="{x:Type Button}">
		<Setter Property="BorderThickness" Value="0"/>
		<Setter Property="FontWeight" Value="SemiBold"/>
		<Setter Property="Foreground" Value="White"/>
		<Setter Property="FontSize" Value="14"/>
		<Setter Property="Padding" Value="30,0,30,0"/>
		<Setter Property="Height" Value="22"/>
        <Setter Property="Margin" Value="6,0,6,0"/>
        
		<Setter Property="Background">
			<Setter.Value>
				<VisualBrush TileMode="Tile">
					<VisualBrush.Visual>
						<StackPanel>
							<StackPanel.Background>
								<DrawingBrush>
									<DrawingBrush.Drawing>
										<GeometryDrawing>

											<GeometryDrawing.Brush>
												<LinearGradientBrush StartPoint="1,0" EndPoint="1,1">
													<GradientStop Color="#7b17d2" Offset="0.0" />
													<GradientStop Color="#c71494" Offset="1" />
												</LinearGradientBrush>
											</GeometryDrawing.Brush>

											<GeometryDrawing.Geometry>
												<GeometryGroup>
													<RectangleGeometry Rect="0,0,100,14" />
													<RectangleGeometry Rect="0,20,100,14" />
													<RectangleGeometry Rect="0,40,100,14" />
													<RectangleGeometry Rect="0,60,100,14" />
													<RectangleGeometry Rect="0,80,100,14" />
													<RectangleGeometry Rect="0,100,100,14" />
												</GeometryGroup>
											</GeometryDrawing.Geometry>

										</GeometryDrawing>
									</DrawingBrush.Drawing>
								</DrawingBrush>
							</StackPanel.Background>
							<TextBlock FontSize="10pt" Margin="10"></TextBlock>
						</StackPanel>
					</VisualBrush.Visual>
				</VisualBrush>
			</Setter.Value>
		</Setter>
	</Style>


    <Style x:Key="tinyTextBox" TargetType="{x:Type TextBox}">
        <Setter Property="Height" Value="22"/>
        <Setter Property="TextWrapping" Value="Wrap"/>
        <Setter Property="Width" Value="40"/>
    </Style>

    <Style x:Key="largeTextBox" TargetType="{x:Type TextBox}">
        <Setter Property="Height" Value="22"/>
        <Setter Property="TextWrapping" Value="Wrap"/>
        <Setter Property="Width" Value="250"/>
    </Style>

    <Style x:Key="windowsTrollLabel" TargetType="{x:Type Label}">
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="Height" Value="31"/>
        <Setter Property="Width" Value="150"/>
        <Setter Property="FontSize" Value="16"/>
    </Style>

    <Style x:Key="windowsTrollElementsWrapper" TargetType="{x:Type WrapPanel}">
        <Setter Property="Orientation" Value="Horizontal"/>
        <Setter Property="Height" Value="auto"/>
        <Setter Property="Width" Value="auto"/>
        <Setter Property="Margin" Value="0,0,0,20"/>
    </Style>

    <Style x:Key="windowsTrollWrapper" TargetType="{x:Type WrapPanel}">
        <Setter Property="Orientation" Value="Vertical"/>
        <Setter Property="Height" Value="auto"/>
        <Setter Property="Width" Value="auto"/>
    </Style>

    <Style x:Key="whiteLineTriangle" TargetType="{x:Type Polygon}">
        <Setter Property="Points" Value="21.5,128.7 97.5,36 178.5,164 "/>
        <Setter Property="Stroke" Value="White"/>
        <Setter Property="StrokeThickness" Value="4"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalAlignment" Value="Bottom"/>
    </Style>

    <Style x:Key="windowsTrollBackground" TargetType="{x:Type Rectangle}">
        <Setter Property="Fill" Value="#141318"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="Height" Value="400"/>
        <Setter Property="Width" Value="500"/>
        <Setter Property="VerticalAlignment" Value="Bottom"/>

        <Setter Property="Margin" Value="20,20,20,20"/>
    </Style>

    <Style x:Key="windowsTrollsStylingWrapper" TargetType="{x:Type DockPanel}">
        <Setter Property="Width" Value="580"/>
        <Setter Property="Margin" Value="20,10,20,20"/>
    </Style>
    
    


</ResourceDictionary>